
public class testConcat {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String noon = "love nos";
		String nos = "love noon";
		String love = noon + nos;
		String love2 = noon.concat(nos);
		System.out.println(love);
		System.out.println(love2);
		int test = love.indexOf("n");
		System.out.println(test);
		String[] test2 = love.split(" ");
		System.out.println(test2);
		
		String kuy = "Hello,World";
		
		String[] name = kuy.split(",");

		for(int i = 0; i < name.length; i++) {
		    System.out.println(name[i]);
		}
		
		int equal = love.compareTo(love2);
		System.out.println(equal);
	}

}
